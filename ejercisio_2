#include<iostream> 
using namespace std;
template<typename L> 
class Alumno{
L dni , anio;
 L nombre;
  
  public:
  	Alumno(L b, L c){
  	  		
  		dni=b;
  		anio=c;
  		
  		
	  }
	  
	Alumno(L a){
	  	nombre=a;
	}
	  
	  
	  void print();
	  
 }	  ;
 template<typename L> 
 void Alumno<L>::print(){
 	cout<<"DNI es: "<<dni<<"  Y  AÃ±o de nacimiento:"<<anio<<endl;
 	cout<<"su edad actual es: "<<2021-anio<<endl;
 	cout<<"Su nombre es: "<<nombre<<endl;
 };
 
 
 
 
 int main(){
 	
 	Alumno<int>obj1(73517491,2002);
 	obj1.print();

 }
