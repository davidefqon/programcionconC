
#include <iostream>

using namespace std;

//PLANTILLA NUEVA CLASE A EJERSICIO ----------------------------------------------------
template<typename  T>

class A{
    T dato;
public:
    A();
    A(T);   
    void print();
    
    
};

template<typename  T>
A<T>::A(){}
template<typename  T>
A<T>::A(T d):dato(d){}

template<typename  T>
void A<T>::print(){
    cout<<dato<<"\n";
}

//PLANTILLA NUEVA CLASE ALUMNO EJERSICIO B----------------------------------------------------

template<typename L> 
class Alumno{
L dni , anio;

  
  public:
  	Alumno(L b, L c){
  	  		
  		dni=b;
  		anio=c;
  		
  		
	  }
	  

	  void print();
	  
 }	  ;
 template<typename L> 
 void Alumno<L>::print(){
 	cout<<"DNI es: "<<dni<<"  Y  AÃ±o de nacimiento:"<<anio<<endl;
 	cout<<"su edad actual es: "<<2021-anio<<endl;
 	
 };


//include a.h

//ESTO VA PARA EL EJERSICIO 3 MOSTRARRRRRR
int main()
{
    //usando la plantilla A
    
    A <string> a("YORDAN");
    a.print();
    
    
    
     //usando la plantilla Alumno
     
    Alumno<int>obj1(73517491,2002);
    obj1.print();

    
    return 0;
}
